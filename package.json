  "bugs": {
    "url": "https://github.com/nf-core/nf-co.re/issues"
  },
  "homepage": "https://github.com/nf-core/nf-co.re#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "backstopjs": "^5.3.4",
    "bootstrap": "^5.0.2",
    "bootstrap-dark-5": "^1.0.2",
    "datatables.net": "^1.10.25",
    "datatables.net-bs5": "^1.10.25",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.2.0",
    "sass": "^1.29.0"
  }
